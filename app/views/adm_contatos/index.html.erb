	<div id="Breadcrumb">
		administração > <a href="/adm_contatos/">contatos</a>
	</div>
	<div id="Content">
		<span class="Titulo">Contatos</span>
		<div class="Linha"></div>
		<div class="Toolbar">
			<li class="Help"><a href="#" id="btnAjuda">Ajuda</a></li>
		</div>
		<%= render :file  => "layouts/_admhelp.html.erb" %>
		<div id="Grid">
			<table width="100%" cellspacing="0" cellpadding="0" border="0">
			<% @contatos.each do |c| %>
			<tr class="bgCinzaC">
			<td class="Name"><li class="Bullet"><%=link_to c.nome + " - " + c.created_at.to_s, adm_contato_path(c) %></li> </td>
			<td class="Actions">
				<li class="Del"><%= link_to "Excluir", adm_contato_path(c), :method => :delete, :confirm => "Tem certeza?"%></li>
			</td>
			</tr>
			<% end %>
			</table>
		</div>
		<div class="Linha"></div>
	</div>
